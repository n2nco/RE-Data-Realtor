   "version": "0.2.0",
   "configurations": [
   



      {
         "type": "pwa-node",
         "request": "launch",
         "name": "Run NodeJS Calculate",
         "skipFiles": [
            "<node_internals>/**"
         ],
         "program": "${workspaceFolder}/calculate.js",
         "runtimeArgs": [
            "--harmony",
            //"set testing=yes",
        ],
      },
      {
         "type": "pwa-node",
         "request": "launch",
         "name": "Run NodeJS Stats Server",
         "skipFiles": [
            "<node_internals>/**"
         ],
         "program": "${workspaceFolder}/server/index.js",
         "runtimeArgs": [
            "--harmony",
            //"set testing=yes",
        ],
      },
      {
         "type": "pwa-node",
         "request": "launch",
         "name": "Run NodeJS Index",
         "skipFiles": [
            "<node_internals>/**"
         ],
         "program": "${workspaceFolder}/index.js",
         "runtimeArgs": [
            "--harmony",
            //"set testing=yes",
        ],
      }
   ]
}